    {% extends 'ThemeBundle:Default:layout_base.html.twig' %}
{% set title = 'Edicion ResultadoCabecera' %}
{% block title %}
{{ title }}
{% endblock %}

    {% block body -%}

    <h4 class="page-header">Resultado Cabecera</h4>
    <div class="row">
        <div class="col-xs-12 col-sm-12">            
            <div class="box-header">
                <div class="box-name ui-draggable-handle">
                    <i class="fa fa-search"></i>
                    <span>Alta de ResultadoCabecera</span>
                </div>
            </div>
            <div class="box-content">
                {#<h4 class="page-header">{{ title }}</h4>#}
                <form class="form-horizontal" action="{{ path('resultado_cabecera_update', { 'id': entity.id }) }}" role="form" method="post">

                        <div class="form-group has-feedback">
                            {{ form_label(form.fecha) }}
                            <div class="col-sm-4">
                                {{ form_widget(form.fecha) }}
                            </div>
                            {{ form_label(form.idExterna) }}
                            <div class="col-sm-4">
                                {{ form_widget(form.idExterna) }}
                            </div>

                        </div>
                        <div class="form-group">
                            {{ form_label(form.infoExterna) }}
                            <div class="col-sm-4">
                                {{ form_widget(form.infoExterna) }}
                            </div>
                            {{ form_label(form.nroCuestionario) }}
                            <div class="col-sm-4">
                                {{ form_widget(form.nroCuestionario) }}
                            </div>
                        </div>
                        {{ form_widget(form) }}

                    <div class="form-group">
                        <div class="col-sm-9 col-sm-offset-3">
                            <button class="btn btn-primary" type="submit"><i class="fa fa-save"></i> Guardar</button>

                            <a class="btn btn-success" href="{{ path('resultado_cabecera') }}">
                                Volver
                            </a>

                        </div>
                    </div>
                </form>


            </div>
        </div>
    </div>


            {% endblock %}


